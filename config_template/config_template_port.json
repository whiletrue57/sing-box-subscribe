                "direct",
                "Proxy"
            ],
            "default": "Proxy"
        },
        {
            "tag": "Global",
            "type": "selector",
            "outbounds": [
                "auto",
                "direct",
                "Proxy"
            ],
            "default": "Proxy"
        },
        {
            "tag": "AdBlock",
            "type": "selector",
            "outbounds": [
                "block",
                "direct"
            ]
        },
        {
            "tag": "auto",
            "type": "urltest",
            "outbounds": [
                "{all}"
            ],
            "url": "http://www.gstatic.com/generate_204",
            "interval": "10m",
            "tolerance": 200
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "dns",
            "tag": "dns-out"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
